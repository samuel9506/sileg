	<queryString language="SQL">
		<![CDATA[SELECT
     productos.`idproductos` AS productos_idproductos,
     productos.`nombreProducto` AS productos_nombreProducto,
     productos.`presentacion` AS productos_presentacion,
     productos.`existencias` AS productos_existencias,
     productos.`precioCompra` AS productos_precioCompra,
     productos.`puntos` AS productos_puntos,
     productos.`fk_categoria` AS productos_fk_categoria,
     productos.`precioVenta` AS productos_precioVenta,
     categoria.`nombre` AS categoria_nombre
FROM
     `categoria` categoria INNER JOIN `productos` productos ON categoria.`idcategoria` = productos.`fk_categoria`]]>
